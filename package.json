{
  "name": "zelda-play",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "~10.14.0",
    "npm": "~6.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format && bash scripts/bin/gitAddMod.sh"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmcdo29/zeldaplay"
  },
  "scripts": {
    "build": "npm run clean && concurrently -n \"client,server\" -c \"green,cyan\" \"npm run build:client\" \"npm run build:server\"",
    "build:client": "ng build --prod",
    "build:server": "tsc -p ./src/server/tsconfig.app.json",
    "clean": "rimraf dist && rimraf coverage",
    "failure": "echo Oops! There was a problem compiling!",
    "format": "prettier --write \"{src/**/*.{ts,scss,html,json},*.{json,yml,yaml},.*.{yml,json,yaml}}\"",
    "heroku-postbuild": "npm run build",
    "lint": "concurrently -n \"client,server\" -c \"green\" \"npm run lint:client\" \"npm run lint:server\"",
    "lint:client": "ng lint",
    "lint:server": "concurrently -n \"main,tests\" -c \"cyan,red\" \"npm run lint:server:src\" \" npm run lint:server:test\"",
    "lint:server:src": "tslint -p src/server/tsconfig.app.json",
    "lint:server:test": "tslint -p src/server/tsconfig.spec.json",
    "ng": "ng",
    "start": "env NODE_ENV=production node -r ./scripts/node/ts-config-bootstrap.js dist/server/main.js",
    "start:debug": "tsc-watch --onSuccess \"node --inspect -r ./scripts/node/ts-config-bootstrap.js dist/server/main.js\"\" --onFailure \"npm run failure\" -p ./src/server/tsconfig.app.json",
    "start:dev": "concurrently -n \"client,server\" -c \"green,magenta\" \"npm run start:dev:client\" \"npm run start:dev:server\"",
    "start:dev:client": "ng serve --open",
    "start:dev:server": "tsc-watch --onSuccess \" node -r ./scripts/node/ts-config-bootstrap.js dist/server/main.js\" --onFailure \"npm run failure\" -p ./src/server/tsconfig.app.json",
    "test": "snyk test && jest --notify",
    "test:ci": "jest --runInBand"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^5.1.1",
    "@nest-middlewares/express-session": "^5.1.1",
    "@nest-middlewares/helmet": "^5.1.1",
    "@nest-middlewares/morgan": "^5.1.1",
    "@nestjs/common": "^5.5.0",
    "@nestjs/core": "^5.5.0",
    "@nestjs/jwt": "^0.2.1",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "compression": "^1.7.3",
    "connect-redis": "^3.4.0",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "file-saver": "^2.0.0-rc.4",
    "googleapis": "^34.0.0",
    "hammerjs": "^2.0.8",
    "helmet": "^3.14.0",
    "jsonwebtoken": "^8.3.0",
    "mc-scribe": "1.2.2",
    "morgan": "^1.9.1",
    "node-notifier": "^5.2.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "tsconfig-paths": "^3.6.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@abraham/reflection": "^0.4.2",
    "@angular-builders/jest": "^7.2.0",
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^7.2.1",
    "@angular/cli": "^7.2.1",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/language-service": "^7.2.0",
    "@angular/material": "^7.2.1",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/router": "^7.2.0",
    "@nestjs/schematics": "^5.11.2",
    "@nestjs/testing": "^5.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "0.0.36",
    "@types/connect-redis": "0.0.8",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/file-saver": "^2.0.0",
    "@types/helmet": "0.0.42",
    "@types/jest": "^23.3.12",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.18",
    "@types/passport-jwt": "^3.0.1",
    "@types/pg": "^7.4.11",
    "@types/supertest": "^2.0.7",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "codecov": "^3.1.0",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "jest-zone-patch": "0.0.10",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "snyk": "^1.122.3",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsc-watch": "^1.0.31",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-sonarts": "^1.8.0",
    "typescript": "^3.2.2"
  }
}

    "snyk": "snyk",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "env LOG_LEVEL=OFF ng test --passWithNoTests",
    "lint": "nx lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",

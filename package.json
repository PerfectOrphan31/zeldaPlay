  "scripts": {
    "commit": "git-cz",
    "docker:all": "npm run docker:build && npm run docker:api && npm run docker:client",
    "docker:build": "docker build -t jmcdo29/build -f Docker/build.Dockerfile .",
    "docker:api": "docker build -t jmcdo29/tabletop-companion-server -f Docker/api.Dockerfile .",

{
  "name": "zelda-play",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "npm": ">=6.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettier && node gitAddMod"
    }
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmcdo29/zeldaplay"
  },
  "scripts": {
    "ng": "ng",
    "devStart": "npm run prettier && npm run build-server && npm run start",
    "start": "node dist/server/bin/www.js",
    "build": "npm run prettier && npm run build-client && npm run build-server",
    "build-client": "ng build --prod",
    "build-server": "tsc -p ./server",
    "test": "jest --notify",
    "test:ci": "snyk test && jest --runInBand",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --config .prettierrc.json --write \"**/*.ts\"",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@abraham/reflection": "^0.4.2",
    "angular-in-memory-web-api": "^0.6.1",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^3.3.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "file-saver": "^1.3.8",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.15.2",
    "morgan": "^1.9.1",
    "node-notifier": "^5.2.1",
    "objection": "^1.2.6",
    "pg": "^7.4.3",
    "rxjs": "^6.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.0",
    "@angular/animations": "^6.1.7",
    "@angular/cli": "^6.2.0",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/compiler-cli": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/forms": "^6.1.7",
    "@angular/http": "^6.1.7",
    "@angular/language-service": "^6.1.7",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@angular/router": "^6.1.7",
    "@types/bcryptjs": "^2.4.1",
    "@types/connect-redis": "0.0.7",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/file-saver": "^1.3.0",
    "@types/helmet": "0.0.40",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/knex": "^0.14.24",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.9.4",
    "@types/supertest": "^2.0.5",
    "codelyzer": "^4.4.4",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.6.0",
    "jest-circus": "^23.5.0",
    "jest-preset-angular": "^6.0.0",
    "jest-zone-patch": "0.0.8",
    "prettier": "^1.14.2",
    "proxyquire": "^2.1.0",
    "snyk": "^1.94.0",
    "supertest": "^3.3.0",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^2.7.2"
  }
}

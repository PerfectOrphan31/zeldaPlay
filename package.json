  "scripts": {
    "commit": "git-cz",
    "docker:all": "yarn docker:build && yarn docker:api && yarn docker:client",
    "docker:build": "docker build -t jmcdo29/build -f Docker/build.Dockerfile .",
    "docker:api": "docker build -t jmcdo29/tabletop-companion-server -f Docker/api.Dockerfile .",
    "docker:client": "docker build -t jmcdo29/tabletop-companion -f Docker/tabletop-companion.Dockerfile .",
    "check": "yarn upgrade-interactive",
    "snyk": "snyk",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "env LOG_LEVEL=OFF ng test --passWithNoTests",
    "lint": "nx lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "env LOG_LEVEL=OFF nx affected:test --passWithNoTests",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "snyk-protect": "snyk protect"

{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "#Enums/*": ["app/models/enums/*"],
      "#Races/*": ["app/models/races/*"],
      "#Models/*": ["app/models/*"],
      "#Alert/*": ["app/alert/*"],
      "#Shared/*": ["app/shared/*"],
      "#Mocks/*": ["app/mocks/*"],
      "#Environment/*": ["environments/*"]
    }
  },
  "exclude": ["**/*.spec.ts", "test.ts"]
}

{
  "name": "zelda-play",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "npm": "6.x"
  },
  "husky": {
    "hooks": {
      "pre-add": "npm run prettier",
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "ng": "ng",
    "devStart": "npm run build-server && npm start",
    "start": "node server/out/bin/www.js",
    "build": "npm run build-client && npm run build-server",
    "build-client": "ng build --prod",
    "build-server": "npm run prettier && tsc -p ./server",
    "test": "npm run test-client && npm run test-server",
    "test-client": "jest -c client/jest.config.js",
    "test-server": "jest -c server/jest.config.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "node_modules/.bin/prettier --config .prettierrc.json --write \"**/*.ts\"",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@abraham/reflection": "^0.4.2",
    "angular-in-memory-web-api": "^0.6.1",
    "bcryptjs": "^2.4.3",
    "connect-session-knex": "^1.4.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "file-saver": "^1.3.8",
    "helmet": "^3.13.0",
    "knex": "^0.15.2",
    "morgan": "^1.9.0",
    "objection": "^1.2.2",
    "pg": "^7.4.3",
    "rxjs": "^6.2.2",
    "winston": "^3.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.2",
    "@angular/animations": "^6.1.1",
    "@angular/cli": "^6.1.2",
    "@angular/common": "^6.1.1",
    "@angular/compiler": "^6.1.1",
    "@angular/compiler-cli": "^6.1.1",
    "@angular/core": "^6.1.1",
    "@angular/forms": "^6.1.1",
    "@angular/http": "^6.1.1",
    "@angular/language-service": "^6.1.1",
    "@angular/platform-browser": "^6.1.1",
    "@angular/platform-browser-dynamic": "^6.1.1",
    "@angular/router": "^6.1.1",
    "@types/bcryptjs": "^2.4.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/file-saver": "^1.3.0",
    "@types/helmet": "0.0.38",
    "@types/jest": "^23.3.1",
    "@types/knex": "^0.14.19",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.7",
    "@types/request": "^2.47.1",
    "@types/request-promise": "^4.1.42",
    "@types/supertest": "^2.0.5",
    "@types/winston": "^2.3.9",
    "codelyzer": "^4.4.3",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.5.0",
    "jest-preset-angular": "^6.0.0",
    "jest-zone-patch": "0.0.8",
    "prettier": "^1.14.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^2.9.2"
  }
}

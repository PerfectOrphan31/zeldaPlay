{
  "name": "zelda-play",
  "version": "1.0.0",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn prettier && node gitAddMod"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmcdo29/zeldaplay"
  },
  "scripts": {
    "ng": "ng",
    "start": "node dist/server/main.js",
    "start:dev": "concurrently \"yarn build:server -w\" \"nodemon dist/server/main.js\" ",
    "start:debug": "concurrently \"yarn build:server -w\" \"node --inspect dist/server/main.js\"",
    "build": "yarn prettier && yarn build:client && yarn build:server\"",
    "build:client": "ng build --prod",
    "build:server": "tsc -p ./src/server/tsconfig.app.json",
    "test": "jest --notify",
    "test:ci": "jest --runInBand",
    "lint": "ng lint",
    "prettier": "prettier --write \"{src/**/*.{ts,css,scss,json},*.{json,yml,yaml},.*.{yml,json,yaml}}\"",
    "heroku-postbuild": "yarn build"
  },
  "_moduleAliases": {
    "@Auth": "dist/server/app/auth",
    "@Entity": "dist/server/app/entities",
    "@Character": "dist/server/app/character",
    "@Note": "dist/server/app/character/note",
    "@Skill": "dist/server/app/character/skill",
    "@Spell": "dist/server/app/character/spell",
    "@User": "dist/server/app/user",
    "@Weapon": "dist/server/app/character/weapon"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^5.1.0",
    "@nest-middlewares/express-session": "^5.1.0",
    "@nest-middlewares/helmet": "^5.1.0",
    "@nest-middlewares/morgan": "^5.1.0",
    "@nestjs/common": "^5.3.11",
    "@nestjs/core": "^5.3.11",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "codecov": "^3.1.0",
    "connect-redis": "^3.4.0",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "file-saver": "^1.3.8",
    "helmet": "^3.14.0",
    "jsonwebtoken": "^8.3.0",
    "mc-scribe": "1.2.2",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.1",
    "node-notifier": "^5.2.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.5.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "typeorm": "^0.2.7",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@abraham/reflection": "^0.4.2",
    "@angular-devkit/build-angular": "0.8.5",
    "@angular/animations": "^6.1.10",
    "@angular/cli": "6.2.5",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@nestjs/schematics": "^5.8.0",
    "@nestjs/testing": "^5.3.11",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "0.0.7",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/file-saver": "^1.3.1",
    "@types/helmet": "0.0.42",
    "@types/jest": "^23.3.5",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.11.7",
    "@types/supertest": "^2.0.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.0.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.1",
    "jest-zone-patch": "0.0.8",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "snyk": "1.103.4",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^2.7.2"
  }
}
